{
    "version": 2,
    "builds": [
      {
        "src": "app-documents-ia/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "./app-documents-ia/dist" }
      },
      {
        "src": "back-end-doc-ia/main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "back-end-doc-ia/main.py"
      },
      {
        "src": "/(.*)",
        "dest": "app-documents-ia/dist/$1"
      }
    ]
  }
  